@model IEnumerable<WebApplication2.Models.MysqlClass>

@{ 
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>Index</title>
</head>
<body>
    <table>
        <tr>
            <th>Name</th>
        </tr>
        @foreach (WebApplication2.Models.MysqlClass details in Model)
            {
                <tr>
                    <td>@details.Name</td>
                </tr>
            }
    </table>
</body>
</html>